				    var videoId = videoUrl.match(/(?:[?&]v=|\/embed\/|\/\d\d?\/|\/vi?\/|https?:\/\/(?:www\.)?youtu\.be\/)([^&\n?#]+)/)[1];
				    var thumbnailUrl = "https://img.youtube.com/vi/" + videoId + "/hqdefault.jpg";
