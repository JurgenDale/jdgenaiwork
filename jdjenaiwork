			jQuery('.sidebar_rows .sidebar_titles').click(function(){
				jQuery(this).toggleClass('row_close');
				if(jQuery(this).hasClass('row_close')){
					jQuery(this).parent('.sidebar_rows').find('.sidebar_row_content').hide('slow');
				} else {
					jQuery(this).parent('.sidebar_rows').find('.sidebar_row_content').show('slow');
				}
			});
			//App filter ajax 
			if (window.innerWidth > 767) {
			    setTimeout(function() {
			        jQuery('#huey').trigger('click');
			    }, 0);
			}
			setTimeout(function(){ 
				jQuery('#load-more-button').trigger('click');
		  	}, 2000);
			jQuery('.sidebar_rows input').click(function(){
				var sortvalues = jQuery('input[name=sorting]:checked', '.sidebar_rows').val();				
				var sortOptios = '';
				if(sortvalues !=='' && typeof sortvalues !== "undefined"){sortOptios = sortvalues;}
				var numberOfChecked = jQuery('.sidebar_rows input:checkbox:checked').length;
				if(numberOfChecked<1 && typeof sortvalues === "undefined"){location.reload();}
				var taxename ='';
				if(jQuery(this).hasClass('category_input')){
					taxename = 'app_categories';
				} else if(jQuery(this).hasClass('tag_input')){
					taxename = 'app_tags';
				}
				var tagArr = new Array();
				var priceArr = new Array();
				var ratingArr = new Array();
				jQuery('.sidebar_rows input[type=checkbox]:checked').each(function() {
				  	var tagids = jQuery(this).attr('value');
			  	 	if(jQuery.isNumeric(tagids)){	
			  	 		var floatValue = parseFloat(tagids);
      					var decimalPart = floatValue % 5;
						if (this.checked && tagids <= decimalPart) {
							ratingArr.push(tagids);
			          	}else{
			          		tagArr.push(tagids);
			          	} 
		          	} else if (!jQuery.isNumeric(priceArr)) {
		          		if (this.checked) {
							priceArr.push(tagids);
			          	} 
		          	}
				});
                jQuery('.top_featured_slider').slick('unslick');
                var opts = {
			   		dots: false,
				 	infinite:true,
					autoplay:true,  
					autoplaySpeed:5000, 		
				  	speed: 800,
				  	slidesToShow:3,
				  	slidesToScroll: 1,
				  	responsive: [
					    {
					      	breakpoint: 1024,
					      	settings: {
								autoplay:true,  
								autoplaySpeed:3000,   
						        slidesToShow: 2,
						        slidesToScroll: 1,
						        infinite: true,
						        dots: true
						      }
					    },
					    {
					      	breakpoint: 767,
					      	settings: {
								autoplay:true,  
								autoplaySpeed:3000,  
						        slidesToShow: 2,
						        slidesToScroll: 1
					      	}
					    },
					    {
					      	breakpoint: 480,
					      	settings: {
								autoplay:true,  
								autoplaySpeed:3000,  
						        slidesToShow: 1,
						        slidesToScroll: 1
					      	}
					    }
				  	]
				};

				//Filter result right box
				var formData = {
                    'action':'app_fliter_listing',
                    'texonomy':taxename, 
                    'filterTagIds': tagArr,
                  	'filterPrice': priceArr,
                  	'filterRating': ratingArr, 
                  	'sortType': sortOptios, 
                };
				jQuery.ajax({
                    type        : 'POST',
                    datatype    : 'html', 
                    url         : Genai_Ajax.ajaxurl, 
                    data        : formData,
                    beforeSend: function() {
			        	jQuery('.loader_container').show();
				    },
                    success:function(res){
                        setTimeout(function(){
				        	jQuery('.loader_container').hide();
				        },1000);
                        var get_data = JSON.parse(res);
                      	jQuery('.app_filter_zone .result_box_cont').html(get_data.filter_right);
                  	},
                  	complete: function() {
				        //jQuery('.loader_container').hide();
				    },
                }); 	

				//Top featured ajax
                var formDataTopFeatured = {
                    'action':'app_fliter_top_featured',
                    'texonomy':taxename, 
                    'filterTagIds': tagArr,
                  	'filterPrice': priceArr,
                  	'filterRating': ratingArr, 
                  	'sortType': sortOptios, 
                };
				jQuery.ajax({
                    type        : 'POST',
                    datatype    : 'html', 
                    url         : Genai_Ajax.ajaxurl, 
                    data        : formDataTopFeatured,
                    beforeSend: function() {
			        	jQuery('.loader_container').show();
				    },
                    success:function(res){
                        setTimeout(function(){
				        	jQuery('.loader_container').hide();
				        },1000);
                        var get_data = JSON.parse(res);
                      	jQuery('.filter_top_featured .top_featured_slider').html(get_data.top_featured_slider);
                  	},
                  	complete: function() {
				        setTimeout(function(){ jQuery('.top_featured_slider').slick(opts);  },800);
				    },
                }); 

				setTimeout(function(){				
					loadmorefunc();
				},1200);
			});//close clicks																																  
			/****************** Start keyword Filtet form filter left **************************/
			var debounceTimer;
			jQuery("input#keyword").keyup(function() {
				jQuery('#keyword_for_top').val('');
				/*------ ctrl, tab, alt and arrrows disable form keyup--------*/
				var keyCode = event.keyCode || event.which;
			    var key = event.key;
			    var keysToDisable = [17, 18, 9, 37, 38, 39, 40]; // Ctrl, Alt, Tab, Arrow keys
			    if (keysToDisable.indexOf(keyCode) !== -1) {
			        event.preventDefault();
			        return false;
			    }
				/*------ ctrl, tab, alt and arrrows disable form keyup--------*/

				var length_Val = jQuery(this).val().length;
			    var optsaa = {
				   	dots: false,
				  	infinite: false,
				  	speed: 300,
				  	slidesToShow: 3,
				  	slidesToScroll: 3,
				};
				clearTimeout(debounceTimer);
 				debounceTimer = setTimeout(function() {
					if (length_Val >= 2) {
						jQuery('.top_featured_slider').slick('unslick');
						jQuery('.recently_add_slider').slick('unslick');
						jQuery.ajax({
						      type        : 'POST',
						      datatype    : 'html', 
						      url         : Genai_Ajax.ajaxurl, 
						      data: { action: 'post_searched_data', keyword: jQuery('#keyword').val(), field_length: length_Val},
						
							success:function(respose){
							var get_data = JSON.parse(respose);
							// console.log(get_data);
							jQuery('.app_filter_zone .result_box_cont').html(get_data.filter_right);
							jQuery('.filter_top_featured .top_featured_slider').html(get_data.top_featured_slider);
							jQuery('.filter_recently_added_content .recently_add_slider').html(get_data.recent_posts);
							},
					      	complete: function() {
						        setTimeout(function(){ jQuery('body').find('.top_featured_slider').slick(optsaa);  },200);
						        setTimeout(function(){ jQuery('body').find('.recently_add_slider').slick(optsaa);},200);
						    },
						}); 
						setTimeout(function(){				
							loadmorefunc();
						},800);
					} else if (length_Val < 1) {
						jQuery('.top_featured_slider').slick('unslick');
						jQuery('.recently_add_slider').slick('unslick');	
						jQuery.ajax({
					      	type        : 'POST',
					      	datatype    : 'html', 
					      	url         : Genai_Ajax.ajaxurl, 
					      	data: { action: 'post_searched_data', keyword: jQuery('#keyword').val(), field_length: length_Val},
